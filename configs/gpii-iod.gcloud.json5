{
    "type": "gpii.iodServer.config.gcloud",
    "options": {
        "components": {
            "iodServer": {
                "type": "kettle.server",
                "options": {
                    "port": "@expand:kettle.resolvers.env(IOD_PORT)",
                    "components": {
                        "packageServer": {
                            "type": "gpii.iodServer.packageServer",
                            "options": {
                                "components": {
                                    "packageDataSource": {
                                        "type": "gpii.iodServer.packageDataSource",
                                        "options": {
                                            "packageDirectory": "%gpii-iod-server/testData/packages"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "distributeOptions": {
            "packageDataSourceFromEnv": {
                "target": "{that gpii.iodServer.packageDataSource}.options.packageDirectory",
                "record": "@expand:kettle.resolvers.env(IOD_PACKAGES_FOLDER)"
            }
        }
    }
}

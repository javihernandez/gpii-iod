{
    "type": "iod.config.dev",
    "options": {
        "gradeNames": ["fluid.component"],
        "distributeOptions": {
            "iod.packageDataSource.moduleTerms": {
                "record": "kettle.dataSource.file.moduleTerms",
                "target": "{that packageDataSource}.options.gradeNames"
            },
            "iod.packageDataSource": {
                "record": {
                    "path": "%gpii-iod/testData/packageData/%packageName.json",
                    "termMap": {
                        "packageName": "%packageName"
                    }
                },
                "target": "{that packageDataSource}.options"
            },
            "iodServer": {
                "record": 8087,
                "target": "{that iodServer}.options.port"
            }
        }
    },
    "mergeConfigs": [
        "%gpii-iod/configs/gpii-iod.base.json"
    ]
}

{
    "type": "gpii.iod.config.base",
    "options": {
        "components": {
            "iodServer": {
                "type": "kettle.server",
                "options": {
                    "components": {
                        "packageServer": {
                            "type": "gpii.iod.packageServer"
                        }
                    }
                }
            },
            "adminSite": {
                "type": "gpii.iod.admin"
            }
        },
        "distributeOptions": {
            "packageDataSource": {
                target: "{that gpii.iod.packageDataSource}.options.packageDirectory",
                record: "%gpii-iod-server/testData/packages"
            }
        }
    }
}
